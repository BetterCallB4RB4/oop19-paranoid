<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<SplitPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="paranoid.controller.GameOverController">
   <items>
      <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0">
         <children>
            <Label text="GAME OVER">
               <font>
                  <Font size="60.0" />
               </font>
            </Label>
            <Label text="PURTROPPO HAI PERSO LA PARTITA!">
               <font>
                  <Font size="20.0" />
               </font></Label>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <TextField fx:id="txtName">
                     <font>
                        <Font size="20.0" />
                     </font></TextField>
                  <Button fx:id="btnSend" mnemonicParsing="false" onMouseClicked="#btnSendOnClickHandler" text="INVIA">
                     <font>
                        <Font size="20.0" />
                     </font></Button>
               </children>
            </HBox>
            <Button fx:id="btnScore" mnemonicParsing="false" onMouseClicked="#btnScoreOnClickHandler" text="MIGLIORI PUNTEGGI">
               <font>
                  <Font size="20.0" />
               </font></Button>
         </children>
      </VBox>
   </items>
</SplitPane>
