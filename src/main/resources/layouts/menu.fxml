               </VBox.margin>
            </Button>
            <Button fx:id="playYourLvl" mnemonicParsing="false" onMouseClicked="#clickPlayYourLvl" style="-fx-background-radius: 30; -fx-background-color: linear-gradient(to bottom right, #008B7C, #38d8c6);" text="PLAY YOUR LEVEL" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
               <VBox.margin>
                  <Insets left="100.0" />
               </VBox.margin>
            </Button>
